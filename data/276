  "cli": [
    "$Self = Get-ADUser -Identity $Env:Username -Properties *",
    "$EID = $Self.EmployeeId",
    "",
    "Get-ADUser  -Filter \"companyOwnerID -like $EID\"  -Properties * | sort-object -property PasswordLastSet | Format-Table Name,PasswordLastSet -AutoSize -Wrap"
