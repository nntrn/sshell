    "cat <<EOF >index.html",
    "<html>",
    "<body>",
    "<ul>",
    "$(find . | grep html | sed 's/\\.\\///g' | sed 's/^\\([^ ]*\\)/<li><a href=\"\\1\">\\1<\\/a><\\/li>/')",
    "</ul>",
    "</body>",
    "</html>",
    "EOF"
