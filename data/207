  "code": [
    "str='delete delete modified other other other'",
    "lstype=($(echo \"$str\"))",
    "echo '{}' > files.json",
    "",
    "for i in ${lstype[@]}; do",
    "  cat files.json | jq \".$i[.$i|length] |= . + {\\\"date\\\": \\\"$(date +%F%X)\\\"}\" >files.json",
    "done"
