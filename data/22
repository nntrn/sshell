{
  "id": 22,
  "title": "creating a user with pass",
  "created": "2021-03-26T18:34:51Z",
  "modified": "2021-12-30T19:44:08Z",
  "code": [
    "useradd -p $(openssl passwd -1 $PASS) $USER"
  ],
  "example": [
    "USER=noreply",
    "PASS=password",
    "useradd -p $(openssl passwd -1 $PASS) $USER"
  ],
  "output": "",
  "description": "",
  "language": "bash",
  "tags": [
    "coreutils"
  ],
