  "cli": [
    "@echo off",
    "setlocal enableextensions",
    "",
    "set CURRENTDIR=%cd%",
    "",
    "C:",
    "chdir \"%~dp0bin\"",
    "set TERM=",
    "",
    "bash --login -i -c \"eval \\\"cd '$CURRENTDIR'\\\";$SHELL\""
