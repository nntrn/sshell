  "title": "bat file for opening cygwin in current directory",
  "created": "2022-01-04T23:18:52Z",
  "modified": "2022-03-17T00:26:23Z",
  "code": [
    "@echo off",
    "setlocal enableextensions",
    "",
    "set CURRENTDIR=%cd%",
    "",
    "C:",
    "chdir \"%~dp0bin\"",
    "set TERM=",
    "",
    "bash --login -i -c \"eval \\\"cd '$CURRENTDIR'\\\";$SHELL\""
  ],
