  "tags": [
    "utils"
  ],
  "description": [],
  "cli": [
    "case \"$OSTYPE\" in",
    "  solaris*) echo \"SOLARIS\" ;;",
    "  darwin*)  echo \"OSX\" ;; ",
    "  linux*)   echo \"LINUX\" ;;",
    "  bsd*)   echo \"BSD\" ;;",
    "  msys*)  echo \"WINDOWS\" ;;",
    "  cygwin*)  echo \"ALSO WINDOWS\" ;;",
    "  *)    echo \"unknown: $OSTYPE\" ;;",
    "esac"
  ],
  "example": [],
  "output": []
}
