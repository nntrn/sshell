  "cli": [
    "alias c1=\"awk '{print \\$1}'\"",
    "alias c2=\"awk '{print \\$2}'\"",
    "alias c3=\"awk '{print \\$3}'\"",
    "alias c4=\"awk '{print \\$4}'\"",
    "alias c5=\"awk '{print \\$5}'\"",
    "alias c6=\"awk '{print \\$6}'\"",
    "alias c7=\"awk '{print \\$7}'\"",
    "alias c8=\"awk '{print \\$8}'\"",
    "alias c9=\"awk '{print \\$9}'\""
  ],
  "example": [
    "demo=' a1 a2 a3",
    "b1 b2 b3",
    "c1 c2 c3'",
    "",
    "echo \"$demo\" | c1",
    ""
  ],
  "output": [
    "a1",
    "b1",
    "c1"
  ]
