  "code": [
    "eval \"$(",
    "  curl -s https://api.github.com/gists/b787f3e14f28de11af45b1c7ec0ffbbc|",
    "    jq -r '.files | to_entries | .[].value | @sh \"echo \\(.content) | tee \\(.filename)\"'",
    ")\"",
    ""
