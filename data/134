  "tags": [],
  "description": [],
  "cli": [
    "Get-ADUser -Filter 'CN -like \"svc_*dbaas*\"'  -Properties * | Where-Object {$_.companyOwnerId -eq \"314810\"} | Sort-Object -Property PasswordLastSet | Format-Table Name,PasswordLastSet,Description,Modified,LastLogonDate,@{N='MemberOf';E={$_.MemberOf[0]}} -AutoSize -Wrap"
  ],
