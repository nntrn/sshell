  "id": "86",
  "title": "psql table size",
  "modified": "2022-07-20T04:10:47Z",
  "created": "2021-07-27T04:59:01Z",
  "guid": "0c4500e5-64ef-4243-bcd3-45f94a202ded",
  "cli": [
    "psql -U awx -d awx -c \"\\dt+\" -P pager=off |",
    "  awk -F \"|\" '{print $2,$5}' |",
    "  grep \"_\" |",
    "  sort -k2 -rh |",
    "  tee /tmp/awx_table_size.txt"
  ],
  "tags": "db",
  "language": "bash",
  "category": "psql"
}
