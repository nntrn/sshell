  "code": [
    "# 1. Stop all containers",
    "docker stop $(docker ps -aq)",
    "",
    "# 2. Remove all containers",
    "docker rm $(docker ps -aq)",
    "",
    "# 3. Prune system",
    "docker system prune -a --volumes",
    "",
    "# 4. Remove all images",
    "docker rmi $(docker images -q)"
