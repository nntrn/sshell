  "cli": [
    "import yaml, json, sys",
    "sys.stdout.write(yaml.dump(json.load(sys.stdin)))"
  ],
  "example": [
    "# run in shell",
    "cat file.json | python -c 'import yaml, json, sys",
    "sys.stdout.write(yaml.dump(json.load(sys.stdin)))'",
    "",
    "# create alias",
    "alias json2yaml=\"python -c 'import yaml, json, sys",
    "sys.stdout.write(yaml.dump(json.load(sys.stdin)))'\""
