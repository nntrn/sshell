  "tags": [],
  "description": [],
  "cli": [
    "Get-ADUser -Filter 'CN -like \"svc_*dbaas*\"'  -Properties * | Where-Object {$_.companyOwnerId -eq \"314810\"} | Format-Table Name,PasswordLastSet,Description,Modified -AutoSize -Wrap"
  ],
