  "tags": [
    "network"
  ],
  "description": [],
  "cli": [
    "netstat -an | grep ESTABLISHED | awk '{print $5}' | awk -F: '{print $1}' | sort | uniq -c | awk '{ printf(\"%s\\t%s\\t\",$2,$1) ; for (i = 0; i < $1; i++) {printf(\"*\")}; print \"\" }'"
  ],
  "example": [],
  "output": [
    "10.143.6.25     1       *",
    "10.170.24.53    2       **",
    "10.174.116.158  1       *",
    "10.191.3.236    1       *",
    "10.58.67.40     1       *",
    "10.58.77.30     1       *",
    "127.0.0.1       4       ****",
    ""
  ]
