{
  "id": 184,
  "title": "date - get N days ago",
  "created": "2022-01-10T15:55:46Z",
  "modified": "2022-03-23T21:36:44Z",
  "code": [
    "date -d \"$date -7 days\" +\"%Y-%m-%d\""
  ],
  "example": [
    "# date today",
    "$ date +\"%Y-%m-%d\"",
    "2022-03-23",
    "",
    "# date 7 days ago",
    "$ date -d \"$date -7 days\" +\"%Y-%m-%d\"",
    "2022-03-16"
  ],
  "output": "",
  "description": "",
  "language": "bash",
  "tags": [],
  "url": "https://stackoverflow.com/a/53326257/7460613"
}
